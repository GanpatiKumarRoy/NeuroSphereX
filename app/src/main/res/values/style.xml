<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RoundedCorners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/dp_5</item>
    </style>

    <style name="ThemeOverlay.AppTheme.TextInputEditText.Outlined" parent="">
        <item name="colorControlActivated">@color/black_color</item>
    </style>

    <style name="description_text_style2" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/primary_color</item>
        <item name="android:textSize">@dimen/sp_18</item>
        <item name="fontFamily">@font/roboto_regular</item>
    </style>

    <style name="customButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/dp_10</item>
        <item name="android:textSize">@dimen/dp_21</item>
        <item name="android:textColor">@color/white_color</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="cornerRadius">@dimen/dp_10</item>
        <item name="backgroundTint">@color/button_disable_color</item>
    </style>
</resources>